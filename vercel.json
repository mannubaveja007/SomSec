{
  "version": 2,
  "builds": [
    {
      "src": "api/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/detection/(.*)",
      "dest": "api/app.ts?path=detection/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/app.ts?path=$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
